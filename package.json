{
  "name": "wscl",
  "version": "2.0.12",
  "license": "WTFPL",
  "description": "Simple browser WebSocket wrapper with some extra features",
  "repository": "github:farwayer/wscl",
  "author": "farwayer <farwayer@gmail.com>",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "nanoevents": "^9.0.0"
  },
  "scripts": {
    "prepack": "size-limit",
    "publish": "clean-publish"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "clean-publish": "^5.0.0",
    "size-limit": "^11.1.4"
  },
  "keywords": [
    "websocket",
    "websocket-client",
    "socket",
    "ws",
    "wss",
    "reconnect"
  ],
  "size-limit": [
    {
      "path": "src/index.js",
      "import": "{Client}",
      "limit": "624 B"
    }
  ],
  "clean-publish": {
    "fields": [
      "packageManager"
    ]
  },
  "packageManager": "yarn@4.4.1"
}
