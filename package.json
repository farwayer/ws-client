{
  "name": "wscl",
  "version": "1.2.0",
  "license": "WTFPL",
  "description": "Simple browser WebSocket wrapper with some extra features",
  "repository": "farwayer/wscl",
  "author": "farwayer <farwayer@gmail.com>",
  "scripts": {
    "prepack": "yarn cjs && yarn esm && size-limit",
    "cjs": "babel --out-file index.cjs.js index.js",
    "esm": "cross-env ESM=1 babel --out-file index.esm.js index.js"
  },
  "sideEffects": false,
  "type": "module",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "react-native": "index.js",
  "exports": {
    ".": {
      "import": "./index.esm.js",
      "default": "./index.cjs.js"
    }
  },
  "dependencies": {
    "nanoevents": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@size-limit/preset-small-lib": "^8.1.0",
    "cross-env": "^7.0.3",
    "size-limit": "^8.1.0"
  },
  "keywords": [
    "websocket",
    "websocket-client",
    "socket",
    "ws",
    "wss",
    "reconnect"
  ],
  "size-limit": [
    {
      "path": "index.esm.js",
      "import": "{WSClient}",
      "limit": "627 B"
    }
  ]
}
