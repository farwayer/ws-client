{
  "name": "wscl",
  "version": "2.0.6",
  "license": "WTFPL",
  "description": "Simple browser WebSocket wrapper with some extra features",
  "repository": "farwayer/wscl",
  "author": "farwayer <farwayer@gmail.com>",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "nanoevents": "^9.0.0"
  },
  "scripts": {
    "prepack": "size-limit"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "size-limit": "^11.0.2"
  },
  "keywords": [
    "websocket",
    "websocket-client",
    "socket",
    "ws",
    "wss",
    "reconnect"
  ],
  "size-limit": [
    {
      "path": "src/index.js",
      "import": "{Client}",
      "limit": "608 B"
    }
  ],
  "packageManager": "yarn@4.1.0"
}
