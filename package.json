{
  "name": "wscl",
  "description": "Simple browser WebSocket wrapper with some extra features",
  "version": "1.0.1",
  "license": "WTFPL",
  "author": "farwayer <farwayer@gmail.com>",
  "repository": "farwayer/wscl",
  "keywords": [
    "websocket",
    "websocket-client",
    "socket",
    "ws",
    "wss",
    "reconnect"
  ],
  "scripts": {
    "prepack": "yarn cjs && yarn esm",
    "cjs": "babel --out-file index.cjs.js index.js",
    "esm": "ESM=1 babel --out-file index.esm.js index.js"
  },
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "nanoevents": "^1.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "caniuse-api": "^3.0.0"
  }
}
